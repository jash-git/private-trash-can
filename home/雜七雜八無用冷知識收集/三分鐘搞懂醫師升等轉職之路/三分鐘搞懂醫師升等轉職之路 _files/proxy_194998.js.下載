// Get URL Parameters
function c194998_getURLParam(param) {
	var strHref = self.location.href;
	if (strHref.indexOf("?") > -1) {
		return decodeURIComponent((new RegExp("[?|&]" + param + "=" + "([^&;]+?)(&|#|;|$)").exec(strHref) || [, ""])[1].replace(/\+/g, "%20")) || "";
