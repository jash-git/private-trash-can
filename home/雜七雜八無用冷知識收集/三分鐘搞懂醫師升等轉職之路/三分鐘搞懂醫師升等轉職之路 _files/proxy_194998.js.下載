	} catch(e) {
		this.IOSVersion = 1;
	}
	// Android
	this.isAndroid = (/(android)/i.test(this.ua));
	try {
		this.AndroidVersion = (this.isAndroid) ? parseFloat(this.ua.slice(this.ua.indexOf("android") + 8)) : 999;
