	if (this.isOpera) {
		this.isIE = false;
	}
	this.isSafari    = (this.ua.indexOf("applewebkit") > 0);
	this.isChrome    = (this.ua.indexOf("chrome") > 0);
	this.isKonqueror = (this.ua.indexOf("konqueror") > 0);
	this.isGecko     = (this.ua.indexOf("gecko/") > 0);
